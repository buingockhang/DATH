                    }, function (result) {
                        //$("span").html(result);
                        //alert(result);
                        if (result == "OK") {
                            $("#form").html('<div class="alert alert-success"><strong>Chúc mừng!</strong> Bạn đã gửi thành công !</div>');
                        } else {
                            $("#form").html('<div class="alert alert-warning"><strong>Xin lỗi!</strong> Bạn đã gửi thất bại !</div>');
                        }
                    });
                });
